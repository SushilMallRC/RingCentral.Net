  "scripts": {
    "definitions": "rm -rf ../RingCentral.Net/Definitions/* && node -r @babel/register definitions.js",
    "definitions-golang": "node -r @babel/register definitions-golang.js",
    "paths": "rm -rf ../RingCentral.Net/Paths/* && node -r @babel/register paths.js",
    "samples": "node -r @babel/register samples.js",
    "samples-java": "node -r @babel/register samples-java.js",
    "samples-typescript": "node -r @babel/register samples-typescript.js",
    "generate": "yarn definitions && yarn paths && yarn samples && yarn samples-java && yarn samples-typescript",
    "validate": "node -r @babel/register validate.js",
    "new-defi": "rm -rf ../RingCentral.Net/Definitions/* && ts-node src/definitions.ts",
    "new-path": "rm -rf ../RingCentral.Net/Paths/* && ts-node src/paths.ts"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "js-yaml": "^4.0.0",
    "lower-case-first": "^2.0.2",
    "ramda": "^0.27.1",
    "ringcentral-open-api-parser": "^0.2.5",
    "to-start-case": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.8",
    "@types/node": "^14.14.35",
    "@types/ramda": "^0.27.39",
    "gts": "^3.1.0",
    "standard": "^16.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "yarn-upgrade-all": "^0.5.4"
  }
}
